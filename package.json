    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-pdf": "^5.0.0",
    "react-scripts": "3.4.0",
    "react-slick": "^0.27.10",
    "rn-fetch-blob": "^0.12.0",
    "slick-carousel": "^1.8.1"
  },
  "eslintConfig": {
